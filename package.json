  "scripts": {
    "dev": "concurrently \"npm run start\" \"npm run styleguide\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "deploy": "npm run build && cp -TRv ./build ../hypersprite.github.io/base-material-components",
    "eject": "react-scripts eject",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "styleguide:deploy": "npm run styleguide:build && cp -TRv ./styleguide ../hypersprite.github.io/base-material-components-styleguide",
    "deploy:all": "npm run styleguide:deploy && npm run deploy"
  }
